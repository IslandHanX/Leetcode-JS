/**
 * @param {number} numRows
 * @return {number[][]}
 */
var generate = function(numRows) {
    const res = [];
    for (let r = 0; r < numRows; r++)
    {
        const row = Array(r + 1).fill(1);
        // 每一层进来先填成1
        for (let c = 1; c < r; c++)
        {
            row[c] = res[r - 1][c - 1] + res[r - 1][c];
            // 只更新中间的格子，第一行第二行不会进入循环，因为需要c < r(r = 0, r = 1)
        }
        res.push(row);
    }
    return res;
};