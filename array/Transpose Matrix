/**
 * @param {number[][]} matrix
 * @return {number[][]}
 */
var transpose = function(matrix) {
    let m = matrix.length, n = matrix[0].length;
    const res = Array.from({ length: n }, () => Array(m));
    for (i = 0; i < m; i++)
    {
        
        for (j = 0; j < n; j++)
        {
            res[j][i] = matrix[i][j];
        }
    }
    return res;
};