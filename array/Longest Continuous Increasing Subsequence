/**
 * @param {number[]} nums
 * @return {number}
 */
var findLengthOfLCIS = function(nums) {
    let ans = 1, cur = 1;
    for (let i = 1; i < nums.length; i++)
    {
        if (nums[i] > nums[i - 1])
        {
            cur++;
        }
        else 
            cur = 1;
        if (cur > ans)
            ans = cur;
    }
    return ans;
};