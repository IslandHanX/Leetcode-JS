/**
 * @param {number[]} cost
 * @return {number}
 */
var minCostClimbingStairs = function(cost) {
    let n = cost.length;
    let dp2 = 0;
    let dp1 = 0;
    for (let i = 2; i <= n; i++)
    {
        const dpi = Math.min(dp1 + cost[i - 1], dp2 + cost[i - 2]);
        dp2 = dp1;
        dp1 = dpi;
    }
    return dp1;
};