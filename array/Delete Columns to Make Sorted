/**
 * @param {string[]} strs
 * @return {number}
 */
var minDeletionSize = function(strs) {
    let m = strs.length, n = strs[0].length;
    let ans = 0;
    for (let i = 0; i < n; i++)
    {
        for (let j = 1; j < m; j++)
        {
            if (strs[j - 1][i] > strs[j][i])
            {
                ans++;
                break;
            }
        }
    }
    return ans;
};
// 二维网格，列检查