/**
 * @param {string} num1
 * @param {string} num2
 * @return {string}
 */
var addStrings = function(num1, num2) {
    let d = 0;
    let res = [];
    let m = num1.length - 1, n = num2.length - 1;
    while (m >= 0 || n >= 0 || d)
    {
        let sum = d;
        if (m >= 0)
            sum += num1.charCodeAt(m--) - 48;
        if (n >= 0)
            sum += num2.charCodeAt(n--) - 48;
        res.push(sum % 10);
        d = Math.floor(sum / 10);
    }
    return res.reverse().join('');
};