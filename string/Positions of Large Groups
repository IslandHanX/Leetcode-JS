/**
 * @param {string} s
 * @return {number[][]}
 */
var largeGroupPositions = function(s) {
    let array = [];
    let i = 0;
    while (i < s.length)
    {
        if (s[i] === s[i + 1] && s[i + 1] === s[i + 2])
        {
            let start = i;
            while (s[i] === s[i + 1])
            {
                i++;
            }
            let end = i;
            array.push([start, end]);
            continue;
        }
        i++;
    }
    return array;
};