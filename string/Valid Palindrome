/**
 * @param {string} s
 * @return {boolean}
 */
var isPalindrome = function(s) {
    let arr = [];
    for (let i = 0; i < s.length; i++)
    {
        if ((s[i].charCodeAt() <= 122 && s[i].charCodeAt() >= 97) || 
            (s[i].charCodeAt() <= 90 && s[i].charCodeAt() >= 65) ||
            (s[i].charCodeAt() <= 57 && s[i].charCodeAt() >= 48))
            arr.push(s[i].toLowerCase());
    }
    if (arr.length === 0)
        return true;
    let tmp = arr.length - 1;
    for (let j = 0; j < tmp; j++)
    {
        if (arr[j] !== arr[tmp])
            return false;
        tmp--;
    }
    return true;
};