/**
 * @param {number[]} nums
 * @return {number[]}
 */
var findErrorNums = function(nums) {
    let t = new Map();
    let ans1 = 0, ans2 = 0;
    for (let nb of nums)
    {
        t.set(nb, (t.get(nb) || 0) + 1);
    }
    for (let i = 1; i <= nums.length; i++)
    {
        if (!(t.has(i)))
            ans2 = i;
        else if (t.get(i) === 2)
            ans1 = i;
    }
    return [ans1, ans2];
};