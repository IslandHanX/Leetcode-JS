/**
 * @param {number} n
 * @return {boolean}
 */
var isPowerOfThree = function(n) {
    if (n === 3 || n === 1)
        return true;
    else if (n < 3)
        return false;
    while (n >= 3)
    {
        if (n % 3 === 0)
            n /= 3;
        else
            break;
    }
    if (n !== 1)
        return false;
    return true;
};