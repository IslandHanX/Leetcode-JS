/**
 * @param {number} n
 * @return {boolean}
 */
var isPowerOfFour = function(n) {
    if (n === 4 || n === 1)
        return true;
    else if (n < 4)
        return false;
    while (n >= 4)
    {
        if (n % 4 === 0)
            n /= 4;
        else
            break;
    }
    if (n !== 1)
        return false;
    return true;
};