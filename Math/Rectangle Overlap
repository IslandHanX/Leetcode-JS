/**
 * @param {number[]} rec1
 * @param {number[]} rec2
 * @return {boolean}
 */
var isRectangleOverlap = function(rec1, rec2) {
    const xOL = Math.max(rec1[0], rec2[0]) < Math.min(rec1[2], rec2[2]);
    const yOL = Math.max(rec1[1], rec2[1]) < Math.min(rec1[3], rec2[3]);

    return xOL && yOL;
};