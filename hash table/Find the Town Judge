/**
 * @param {number} n
 * @param {number[][]} trust
 * @return {number}
 */
var findJudge = function(n, trust) {
    if (n === 1 && trust.length === 0)
        return 1;
    const score = Array(n + 1).fill(0);
    for (const [a, b] of trust)
    {
        score[a]--;
        score[b]++;
    }
    for (let i = 1; i <= n; i++)
    {
        if (score[i] === n - 1)
            return i;
    }
    return -1;
};