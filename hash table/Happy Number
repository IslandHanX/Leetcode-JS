/**
 * @param {number} n
 * @return {boolean}
 */
var isHappy = function(n) {
    const seen = new Set();
    const next = (x) => {
        let s = 0;
        while (x > 0) {
            const d = x % 10;
            s += d * d;
            x = (x / 10) | 0;
        }
        return s;
    }

    while (n !== 1 && !seen.has(n))
    {
        seen.add(n);
        n = next(n);
    }
    return n === 1;
};