/**
 * @param {string} s
 * @return {number}
 */
var firstUniqChar = function(s) {
    const cnt = new Map();
    for (const ch of s)
    {
        cnt.set(ch, (cnt.get(ch) || 0) + 1);
    }
    for (let i = 0; i < s.length; i++)
    {
        if (cnt.get(s[i]) === 1)
            return i;
    }
    return -1;
};